{
  "functions": {
    "api/*.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "redirects": [
    {
      "source": "/",
      "destination": "https://github.com/Mindhunter86"
    }
  ],
  "headers": [
    {
      "source": "/",
      "headers" : [
        {
          "key" : "X-Robots-Tag",
          "value" : "noindex"
        }
      ]
    }
  ]
}
